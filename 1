num = (int(input("Enter a number: "))

if num <=1:
   printf(num,"is not a prime number.")
else:
   is prime = True
   for i in range(2, int (num ** 0,5) + 1):
       if num % i ==0:
           is prime = False
           break
   
   if is prime:
       print(num, "is a prime number.")
   else:
       print(num, "is not prime number
